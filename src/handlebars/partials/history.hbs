<section class="history enclosure">
  <section class="experience stretchable has-overlay">
    <h3>Experience</h3>
    {{#each experiences as |experience|}}
      {{>project experience}}
    {{/each}}

    {{> overlay isReversed=true}}
  </section>

  <div class="divider"></div>

  <section class="projects stretchable has-overlay">
    <h3>Projects</h3>
    {{#each projects as |project|}}
      {{>project project}}
    {{/each}}

    {{> overlay isReversed=true}}
  </section>
</section>
